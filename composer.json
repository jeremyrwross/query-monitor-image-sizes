{
	"name": "jeremyrwross/query-monitor-image-sizes",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"description": "A Query Monitor extension that adds a tab to show registered image sizes.",
	"homepage": "https://jereross.com/query-monitor-image-sizes/",
	"keywords": [
		"wordpress",
		"plugin",
		"query monitor",
		"image sizes",
		"debugging"
	],
	"authors": [
		{
			"name": "Jeremy Ross",
			"email": "jeremyrwross@gmail.com",
			"homepage": "https://jereross.com"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.9",
		"wp-coding-standards/wpcs": "^3.0",
		"phpcompatibility/php-compatibility": "^9.3",
		"friendsofphp/php-cs-fixer": "^3.51"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
